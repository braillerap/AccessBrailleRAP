{
  "name": "access-braillerap",
  "version": "0.5.1",
  "private": true,
  "dependencies": {
    "electron": "^25.0.1",
    "file-saver": "^2.0.5",
    "purecss": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^6.3.2",
    "react-intl-cli": "^0.1.2",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "python index.py --develop",
    "build:eel": "python -m eel index.py build --onefile --noconsole --name AccessBrailleRAP --clean --icon=favicon-32x32.ico --splash=brap.png",
    "build:webview": "pyinstaller AccessBrailleRAPW.spec",
    "clean:build": "rm -rf build dist",
    "start": "npm-run-all -p start:*",
    "build": "npm-run-all clean:build build:*",
    "buildp": "npm-run-all clean:build build:js",
    "buildweb": "npm-run-all clean:build build:js build:webview",
    "extract": "react-intl-cli  extract -l fr,en -d fr ",
    "extracth": "react-intl-cli  extract -h",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
